<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.epicsOfBharat">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.epicsOfBharat.AboutActivity"></activity>
        <activity android:name="com.example.epicsOfBharat.CurriculumActivity"></activity>
        <activity android:name="com.example.epicsOfBharat.QuestionBankActivity"></activity>
        <activity android:name="com.example.epicsOfBharat.VideosActivity"></activity>
        <activity android:name="com.example.epicsOfBharat.RamayanaActivity"></activity>
        <activity android:name="com.example.epicsOfBharat.MahabharataActivity"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>