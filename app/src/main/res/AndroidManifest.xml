<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.htmlapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.epicsOfBharat.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.epicsOfBharat.Listview" />
        <activity android:name="com.example.epicsOfBharat.AboutActivity" />
        <activity android:name="com.example.epicsOfBharat.CurriculumActivity" />
        <activity android:name="com.example.epicsOfBharat.QuestionBankActivity" />
        <activity android:name="com.example.epicsOfBharat.VideosActivity"></activity>
    </application>

</manifest>